    function SetLanguage() {
      var urlParam = window.location.search;
      if (typeof urlParam === 'undefined') {
        urlParam = '';
      }
      //Create the model
      var model = {
        "language": "en",
        "url": "/staying-well/safety/child/car-seat",
        "sitename": "bh"
      };
	 // console.log(model);

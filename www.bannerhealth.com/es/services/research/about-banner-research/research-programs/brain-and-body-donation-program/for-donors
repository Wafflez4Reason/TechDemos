    function SetLanguage() {
      var urlParam = window.location.search;
      if (typeof urlParam === 'undefined') {
        urlParam = '';
      }
      //Create the model
      var model = {
        "language": "en",
        "url": "/services/research/about-banner-research/research-programs/brain-and-body-donation-program/for-donors",
        "sitename": "bh"
      };

  <script type="text/javascript">


    function SetLanguage() {
      var urlParam = window.location.search;
      if (typeof urlParam === 'undefined') {
        urlParam = '';
      }
      //Create the model
      var model = {
        "language": "en",
        "url": "/about/legal-notices/privacy",
        "sitename": "bh"
      };
	 // console.log(model);
      //call the SetLanguage action method
      $.ajax({
        url: '/api/sitecore/Language/SetLanguage',
        data: model,
        type: "POST",
        cache: false,
        success: function (data) {
          console.log('success:redirect url' + data);
          //Redirect to new url
          window.location.href = data + urlParam;
        },
        failure: function (result) {
          console.log('failure');
          console(result.responseText);
        },
        error: function (result) {
          console.log('error');
          console.log(result.responseText);
        }
      })

    }

  </script>

